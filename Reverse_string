import java.util.*;
public class HelloWorld {
    static String reverse(String s)
    {
        String t = "";
        for(int i=s.length()-1 ; i>=0 ; i--)
        {
            t = t + s.charAt(i);
        }
        return t;
    }
    
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
      
        String s = sc.next();
        
        String res = reverse(s);
        
        System.out.println(res);
